<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="elrik">


  <!-- Blender/Phobos generated description  -->
  <joint name="world_joint" type="fixed">
      <origin xyz="0 0 0.964145" rpy="0 0 0"/>  <!-- 0.964145 is the height of the robot from its origin -->
      <parent link="world"/>
      <child link="link_torso"/>
  </joint>
  <link name="world"/>

  <xacro:include filename="$(find elrik_description)/urdf/phobos_generated_no_wrist_roll.urdf"/>
  <!-- NOTE: Change all relative paths "../" with "file://$(find elrik_description)/" -->
  <!-- Add <dynamics damping="1.0" friction="1.0"/> to all joints -->

</robot>